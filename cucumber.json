{
  "all": {
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "paths": ["tests/features/*"],
    "dryRun": false,
    "require": ["tests/steps/*.ts", "HooksHelper/hooks.ts"],
    "requireModule": ["ts-node/register"],
    "format": [
      "progress-bar",
      "html:test-results/cucumber-report.html",
      "json:test-results/cucumber-report.json"
    ],
    
    "timeout": 3600000,
    "tags": "not @skip"
  }
  
}